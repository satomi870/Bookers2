<%=render 'books/list', users: @user, book: @book%>


  

<h2>Books</h2>

<table>
  <thead>
    <tr>
    <th></th>
    <th>Title</th>
    <th>Opinion</th>
    <th colspan="3"></th>
    </tr>
  </thead>
  <%@books.each do |book|%>
  <tbody>
    <tr>
    <td><%=image_tag book.user.get_profile_image(100,100)%></td>
    <td><%=link_to book.title,book_path(book.id)%></td>
    <td><%=book.body%></td>
    </tr>
  </tbody>
  <%end%>
 </table>

 
  